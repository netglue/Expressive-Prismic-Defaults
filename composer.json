{
    "name": "netglue/expressive-prismic-defaults",
    "description": "Opinionated defaults for setup of a Zend Expressive website backed by Prismic.io CMS",
    "type": "project",
    "license": "MIT",
    "minimum-stability" : "dev",
    "prefer-stable" : true,
    "authors": [
        {
            "name": "George Steel",
            "email": "george@net-glue.co.uk"
        }
    ],
    "require": {
        "netglue/expressive-prismic": "^2.0",
        "zendframework/zend-view": "^2.8"
    },
     "autoload": {
        "psr-4": {
            "ExpressivePrismic\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ExpressivePrismicTest\\": "test/ExpressivePrismicTest"
        }
    },
    "repositories" : [
        {
            "type": "vcs",
            "url": "git@github.com:netglue/expressive-prismic.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^5.5",
        "zendframework/zend-paginator": "^2.7"
    }
}
